{
  "name": "areader",
  "version": "1.3.0",
  "author": "Luke Pan",
  "main": "build/dist/prod/main.js",
  "build": {
    "productName": "镜览",
    "appId": "app.nsplup.areader",
    "copyright": "Copyright 2021 Luke Pan",
    "directories": {
      "buildResources": "build"
    },
    "asar": false,
    "artifactName": "${productName}-installer.${ext}",
    "files": [
      "build/icons/icon.ico",
      "build/dist/prod/**/*"
    ],
    "extraResources": [
      "./build/dependencies/7z/${platform}/${arch}"
    ],
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false,
      "menuCategory": true,
      "allowElevation": false
    },
    "electronDownload": {
      "mirror": "http://npm.taobao.org/mirrors/electron/"
    }
  },
  "scripts": {
    "buildDev": "webpack --config webpack.dev.config.js && electron ./build/dist/dev/main.js",
    "build": "webpack --config webpack.prod.config.js && electron-builder --dir"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@types/node-7z": "^2.1.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-slider": "^1.1.2",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.3",
    "@types/redux": "^3.6.0",
    "@types/redux-saga": "^0.10.5",
    "autoprefixer": "^9.8.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "electron": "13.0.1",
    "electron-builder": "^22.10.5",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "postcss-loader": "^3.0.0",
    "px2-loader": "^1.0.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.4",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "fast-xml-parser": "^3.19.0",
    "font-list": "^1.2.14",
    "iconv-lite": "^0.6.3",
    "jschardet": "^3.0.0",
    "node-7z": "^2.1.2",
    "react": "^16.13.1",
    "react-colorful": "^5.2.0",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-slider": "^1.1.4",
    "react-virtualized-auto-sizer": "^1.0.5",
    "react-window": "^1.8.6",
    "redux": "^4.0.5"
  }
}
